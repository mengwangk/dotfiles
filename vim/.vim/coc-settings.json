{
	"languageserver": {
		"go": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"trace.server": "verbose",
			"filetypes": ["go"]
		}
	},
	"python.jediEnabled": false,
	"python.formatting.provider": "yapf",
	"coc.preferences.formatOnType": true,
	"coc.preferences.formatOnSaveFiletypes": ["python"],
	"explorer.previewAction.onHover": false
}
